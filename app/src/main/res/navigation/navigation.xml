<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">


    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.neoregister.bottomnav.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_cartFragment"
            app:destination="@id/cartFragment" />
    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.example.neoregister.bottomnav.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" />
    <fragment
        android:id="@+id/QRFragment"
        android:name="com.example.neoregister.bottomnav.QRFragment"
        android:label="QRFragment" />
    <fragment
        android:id="@+id/branchesFragment"
        android:name="com.example.neoregister.bottomnav.BranchesFragment"
        android:label="fragment_branches"
        tools:layout="@layout/fragment_branches" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.neoregister.bottomnav.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>